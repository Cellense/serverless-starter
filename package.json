{
  "name": "lambda-functions",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://gitlab.com/buffpanel/lambda-functions",
  "description": "Buffpanel lambda functions",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev npx sls offline start",
    "deploy": "npx sls deploy function --function",
    "deploy:stack": "npx sls deploy",
    "sls:config": "npx sls config credentials --provider aws",
    "logs": "npx sls logs -f"
  },
  "dependencies": {
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.246.1",
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@types/aws-lambda": "^8.10.5",
    "@types/node": "^10.1.2",
    "@types/source-map-support": "^0.4.0",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-node-source-map-support": "^0.0.1",
    "cross-env": "^5.1.6",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "prettier": "^1.12.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "serverless": "^1.27.3",
    "serverless-offline": "^3.23.1",
    "serverless-webpack": "^5.1.5",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-node-externals": "^1.7.2"
  }
}
