{
    "rules": {
      "prettier": [
        true,
        {
          "singleQuote": true,
          "printWidth": 100
        }
      ],
      "no-unused": true,
      "no-unused-variable": [
        true,
        {
          "ignore-pattern": "^_"
        }
      ],
      "variable-name": [
        true,
        "check-format",
        "allow-leading-underscore"
      ],
      "no-console": {
        "severity": "warn",
        "options": [
          "log"
        ]
      },
      "missing-jsdoc": false,
      "typedef": false,
      "no-relative-imports": false,
      "interface-name": false,
      "no-any": false,
      "import-name": false,
      "export-name": false,
      "no-default-export": false,
      "no-implicit-dependencies": false,
      "newline-before-return": false,
      "no-import-side-effect": false,
      "no-submodule-imports": false
    },
    "extends": [
      "tslint-consistent-codestyle",
      "tslint-eslint-rules",
      "tslint-microsoft-contrib",
      "tslint-config-airbnb",
      "tslint-config-prettier"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "linterOptions": {
      "exclude": [
        "node_modules",
        "src/generated/*"
      ]
    }
  }
